{
	"id": "/dployex/buzzcon",
	"dependencies": ["/dployex/buzzgen"],
	"cmd": "curl -OsL https://raw.githubusercontent.com/mhausenblas/dploy/master/examples/simple/buzzcon.py && python3 buzzcon.py",
	"cpus": 0.5,
	"mem": 100,
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "python:3",
			"forcePullImage": true,
			"network": "BRIDGE",
			"portMappings": [
				{
					"containerPort": 8888,
					"hostPort": 0
				}
			]
		}
	},
	"acceptedResourceRoles": [
		"slave_public"
	]
}
