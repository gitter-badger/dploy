{
	"id": "/dployex/sanitizer",
	"dependencies": ["/dployex/buzzgen"],
	"cmd": "curl -OsL https://raw.githubusercontent.com/mhausenblas/dploy/master/examples/simple/sanitizer_bootstrap.sh && curl -OsL https://raw.githubusercontent.com/mhausenblas/dploy/master/examples/simple/sanitizer.py && chmod 755 sanitizer_bootstrap.sh && ./sanitizer_bootstrap.sh",
	"cpus": 0.5,
	"mem": 100,
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "x110dc/dev",
			"forcePullImage": true,
			"network": "BRIDGE",
			"portMappings": [
				{
					"containerPort": 5000,
					"hostPort": 0
				}
			]
		}
	},
	"acceptedResourceRoles": [
		"slave_public"
	]
}
