{
	"id": "buzzgen",
	"cmd": "echo cloud native hyperscale hybrid container microservices architecture >index.html && python3 -m http.server 8888",
	"cpus": 0.5,
	"mem": 32.0,
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "python:3",
			"forcePullImage": true,
			"network": "BRIDGE",
			"portMappings": [
				{
					"containerPort": 8888,
					"hostPort": 0
				}
			]
		}
	},
	"acceptedResourceRoles": [
		"slave_public"
	]
}
